<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
     <Page title="Booking Schedule">
        <content>
            <Button
                text="Fetch Schedule"
                press="onFetchSchedule" />
            <List
                id="scheduleList"
                headerText="Schedule">
                <items>
                    <ObjectListItem
                        title="{date}"
                        number="{time}"
                        numberUnit="{status}" />
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
